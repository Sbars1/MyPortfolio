@model Contact
@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/Layout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İletişim Bilgileri Güncelle </h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Deneyim Güncelleme Sayfası</div>
					</div>
					<div class="card-body">

						<form method="post">
							<input type="hidden" asp-for="ContactId " />
							<label> Telefon</label>
							<input type="text" asp-for="Phone1" class="form-control" />
							<input type="text" asp-for="Phone2" class="form-control" />
							<br />
							<label>Email</label>
							<input type="text" asp-for="Email1" class="form-control" />
							<input type="text" asp-for="Email2" class="form-control" />
							<br />
							<label>Adres</label>
							<textarea rows="4" asp-for="Address" class="form-control"></textarea>
							<br />
							<button class="btn btn-warning">Bilgileri Güncelle</button>

						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>




